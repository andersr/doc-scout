  },
  "private": true,
  "scripts": {
    "build": "react-router build",
    "checks": "run-s format:fix lint:fix typecheck test:run",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "dev": "react-router dev",
    "e2e:codegen": "PORT=8080 dotenv -e .env.test -- npx playwright codegen --load-storage=playwright/.auth/doc_delete.json http://localhost:8080",
    "e2e:db:reset": "dotenv -e .env.test -- prisma migrate reset --force",
    "e2e:db:studio": "dotenv -e .env.test -- prisma studio",
    "e2e:devserver": "PORT=8080 dotenv -e .env.test -- react-router dev",
    "e2e:test": "PORT=8080 dotenv -e .env.test -- run-s e2e:db:reset build pw:test",
    "e2e:ui": "PORT=8080 dotenv -e .env.test -- playwright test --ui",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --write",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:staged": "npx lint-staged",
    "postinstall": "prisma generate",
    "prepare": "husky",
    "pw:test": "playwright test",
    "start": "react-router-serve ./build/server/index.js",
    "start:dev": "PORT=4000 dotenv -e .env.test -- npm start",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "react-router typegen && safe-routes typegen && tsc --build --noEmit"
  },
  "type": "module"
