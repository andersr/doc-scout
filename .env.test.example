### ENV ###
E2E_ENV=true

### AUTH ###
# Generate secret, eg with openssl rand -hex 32
AUTH_SESSION_SECRET=
# Comma-separated emails for test users, or "any" for open access
ALLOWED_USERS=

### ADOBE ###
# Set up a PDF services project. See: https://developer.adobe.com/document-services/apis/pdf-services/
ADOBE_PDF_SERVICES_CLIENT_ID=
ADOBE_PDF_SERVICES_CLIENT_SECRET=

### AWS ### 
# Create an IAM user with AmazonS3FullAccess and AWSCloudFormationFullAccess 
AWS_S3_ACCESS_KEY=
AWS_S3_SECRET=

# S3 bucket for test doc storage (use separate bucket from production)
AWS_DATA_BUCKET_NAME=
AWS_REGION=

### CDN ###
# CDN for test doc web links
AWS_HOST=

### DEV ###
# Your ngrok domain for local e2e testing (optional)
DEV_HOST=

### E2E ###
# Test user password for e2e authentication flows
TEST_USER_PWD=

### PRISMA ###
# Test database connection strings (use separate database from development/production)
DATABASE_URL=postgresql://user:password@localhost:5432/research_tool_test?pgbouncer=true
DIRECT_URL=postgresql://user:password@localhost:5432/research_tool_test

### FIRECRAWL ###
# Get an API key at https://www.firecrawl.dev/ 
FIRECRAWL_API_KEY=

### OPENAI ### 
# Get an API key at https://platform.openai.com/
OPENAI_API_KEY=

### PINECONE ### 
# Create a separate test index for testing
# Get API key and create a vector store at https://www.pinecone.io/
PINECONE_API_KEY=
PINECONE_HOST=
PINECONE_INDEX_NAME=

### STYTCH ###
# Use your Project's 'test' credentials from https://stytch.com/
STYTCH_PROJECT_ID=
STYTCH_SECRET=